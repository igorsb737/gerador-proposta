{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/public/editor.html" },
    { "source": "/visualizar.html", "destination": "/public/visualizar.html" },
    { "source": "/proposta/:id", "destination": "/public/visualizar.html" }
  ],
  "cleanUrls": true
}
