{
  "name": "sistema-proposta",
  "version": "1.0.0",
  "description": "Sistema de edição de propostas comerciais",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build-css-prod",
    "build-css": "tailwindcss -i ./src/input.css -o ./public/styles.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./public/styles.css --minify"
  },
  "dependencies": {
    "express": "^4.18.2",
    "uuid": "^9.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "@vercel/blob": "^0.23.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "keywords": ["proposta", "edição", "comercial"],
  "author": "",
  "license": "ISC"
}
